syntax enable
filetype plugin on
filetype plugin indent off
set nocompatible
set number
set mouse=a
set expandtab
set autoindent
set smartindent
set tabstop=4
set shiftwidth=4
set softtabstop=4
set ruler
set nowrap
set showcmd
set showmode
set autoread
set autochdir
set showmatch
set cursorline
